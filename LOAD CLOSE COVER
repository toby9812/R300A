*223
RINT I200,0,0,0,0
MOVEX A=1,AC=0,SM=0,M1J,P,P223,R=V31%,H=1,MS,M2J,P,P223,R=V33%	'P223
INH
LETVI V19%,223
IF I200=1
GETANGLE V90!
MOVEX A=1,AC=0,SM=0,M1J,L,(V90!,V91!,V92!,V93!,V94!,V95!),S= 150.0,H=1,MS,M2J,P,(V96!),S=150
SETM O214,1
GOTO *END
